    <style id="MENU_LABEL"
	textColor="#000000"
        margins="1,0,1,0"
        align="Left|VCenter"
        textFlags="UnderlineHotKeys"
        >
        <state state_enabled="false" textColor="#C0C0C0"/>
    </style>
    <style id="MAIN_MENU_LABEL"
	textColor="#000000"
        margins="1,0,1,0"
