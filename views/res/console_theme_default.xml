        backgroundImageId="{'░' 0x808000 stretch}"
        >
        <state state_pressed="true" backgroundImageId="{'░' 0x808080 stretch}"/>
        <state state_hovered="true" backgroundImageId="{'░' 0x808000 stretch}"/>
    </style>

