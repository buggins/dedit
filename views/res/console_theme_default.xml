    <style id="MAIN_MENU"
        layoutWidth="FILL_PARENT"
        backgroundImageId="{' ' bc 0x808080 stretch}"
	layoutWeight="0"
	textColor="#000000"
        />
    <style id="MAIN_MENU_ITEM"
        padding="0,0,0,0"
        backgroundImageId="{' ' bc 0x808080 stretch}"
